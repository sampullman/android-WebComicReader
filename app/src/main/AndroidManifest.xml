<?xml version="1.0" encoding="utf-8"?>
<manifest
    package="com.threeDBJ.comicReader"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:installLocation="preferExternal"
    android:versionCode="21"
    android:versionName="4">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application
        android:name="ComicReaderApp"
        android:icon="@drawable/ic_main"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ComicSelectActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".reader.SMBCReader"
            android:label="@string/app_name" />

        <activity
            android:name=".reader.XKCDReader"
            android:label="@string/app_name" />

        <activity
            android:name=".reader.FeelAfraidReader"
            android:label="@string/app_name" />

        <activity
            android:name=".reader.ExplosmReader"
            android:label="@string/app_name" />

        <activity
            android:name=".reader.PHDReader"
            android:label="@string/app_name" />

        <activity
            android:name=".reader.ButtersafeReader"
            android:label="@string/app_name" />

        <activity
            android:name=".reader.QuestionableContentReader"
            android:label="@string/app_name" />

        <activity
            android:name=".reader.DinosaurReader"
            android:label="@string/app_name" />

        <activity
            android:name=".reader.CtrlAltDeleteReader"
            android:label="@string/app_name" />

        <activity
            android:name=".reader.PennyArcadeReader"
            android:label="@string/app_name" />

        <activity
            android:name=".SavedComicReader"
            android:label="@string/app_name" />

        <activity
            android:name=".reader.AbstruseGooseReader" />

        <activity
            android:name=".reader.ManlyGuysReader" />

        <activity
            android:name=".reader.CompletelySeriousReader" />

        <activity
            android:name=".reader.MongrelDesignsReader" />
    </application>
</manifest>
